    <AssemblyVersion></AssemblyVersion>
    <FileVersion></FileVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>3.0</Version>
    <PackageReadmeFile></PackageReadmeFile>
    <Authors>Soheil MV</Authors>
    <Company>MVSoft</Company>
